.play{
    width:7vw;
    height: 7vw;
    background: url(https://cdn.shopify.com/s/files/1/1212/4268/files/Play_Button.svg?10077);
    background-size: cover;
    background-repeat: no-repeat;
    border: none;
    cursor: pointer;
    border-radius: 50%;
    position: absolute;
    top: calc(50% - 3.5vw);
    left: calc(50% - 3.5vw);
    z-index: 100;
}
.pause{
    width: 7vw;
    height: 7vw;
    background: url(https://cdn.shopify.com/s/files/1/1212/4268/files/Play_Button.png?10077);
    background-size: cover;
    background-repeat: no-repeat;
    border: none;
    cursor: pointer;
    border-radius: 50%;
    position: absolute;
    top: calc(50% - 3.5vw);
    left: calc(50% - 3.5vw);
    z-index: 100;
    animation-name: hide;
    animation-duration: 1s;
    animation-fill-mode: forwards;
}
.third-top-video{
	width:100%;
}
.third-top-video:hover .pause{
    animation-name: appear;
    animation-duration: 1s;
    animation-fill-mode: forwards;
}

@keyframes hide {
  from {
    opacity: 1; 
  }

  to {
    opacity: 0;
  }
}
@keyframes appear {
  from {
    opacity: 0; 
  }

  to {
    opacity: 1;
  }
}





















