@font-face {
 font-family: "FreightDispProBook";
  src: url("{{ 'FreightDispProBook-Regular.ttf' | asset_url }}") format("truetype");
}
.spr-icon {
    color: #FFBC32 !important;
}
.best_sell_headline{
    display: flex;
    flex-direction: row;
    justify-content: center;
    width: 100%;
}
.best_sell_headline h1{
    font-family: FreightDispProBook;
    font-size: 60px;
    font-weight: 100;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: normal;
    color: #000000;
    margin: 80px 0 48px 0;
	text-transform:none !important; 
}
.best_sellers_block{
    display: flex;
    flex-direction: row;
    justify-content: center;
    width: 100%;
	padding-bottom: 80px;
    background: linear-gradient(to top, #f4f4f4 0%, #f4f4f4 75%, #fff 75%);
    background-position: bottom center;
    background-repeat: no-repeat;
  margin-top: 80px;
}
.best_sellers_position{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 80%;
	align-items: stretch;
}
.best_seller_item{
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 32%;
    justify-content: space-between;
    background-color: #FFF;
}
.best_seller_item img{
    max-width: 100%;
    height: auto;
    margin-bottom: 40px;
}
.best_seller_item h1{
    font-family: FreightDispProBook;
    font-size: 40px;
    font-weight: 100;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: normal;
    color: #000000;
    margin-bottom: 32px;
    text-align: center;
	text-transform: none !important;
	margin-left: 10%;
    margin-right: 10%;
}
.price_block{
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-bottom: 82px;
	margin-left: 10%;
    margin-right: 10%;
}
.price_block h1{
    font-family: Gotham;
    font-size: 24px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: normal;
    color: #000000;
    margin: 0 20px 0 0;
	padding: 0 !important;
}
.price_block h3{
    font-family: Gotham;
    font-size: 20px;
    font-weight: 100;
    color: #c4c4c4;
    text-decoration: line-through;
	margin: 0 !important;
	padding: 0 !important;
}
.best_seller_item a{
    display: flex;
    justify-content: center;
    font-family: Gotham;
    font-size: 16px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: normal;
    color: #ffffff;
    background-color: #000000;
    text-decoration: none;
    text-transform: uppercase;
    padding: 16px 0;
    width: 80%;
    text-align: center;
	margin-bottom: 40px;
}




