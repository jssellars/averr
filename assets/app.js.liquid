function updateFreeShipping(cart) {
    var total_price = cart.total_price,    	
        freeShippingLimit = 79,
        free_shipping = freeShippingLimit - (total_price  / 100),
        percent = total_price / freeShippingLimit;

    jQuery(".cart_shipping_block").css('display','flex');
    if(free_shipping <=0) {
      jQuery(".cart_shipping_headline").html('{{ 'cart.general.free_shipping_headline' | t }}');
      jQuery(".cart_shipping_info").html('{{ 'cart.general.free_shipping_info_html' | t }}');
    } else {
      jQuery(".cart_shipping_headline").html('{{ 'cart.general.shipping_headline' | t }}');
      jQuery(".cart_shipping_info").html('{{ 'cart.general.shipping_info_html' | t }}');
      $('#left_amount').text(free_shipping);
    }
    $(".cart_shipping_progress").css("background", "linear-gradient(90deg, #f9dbd0 " + percent + "%" + ", rgba(0,0,0,0) " + percent + "%)");
}
