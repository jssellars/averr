if(quantity>0&&quantity!=updated_quantity){if(updated_quantity==1){items_left_text={{'products.product.items_left_count.one'|t|json}}}else{items_left_text={{'products.product.items_left_count.other'|t|json}}}
$('.warning--quantity').remove();var warning='<div class="warning warning--quantity animated bounceIn">'+updated_quantity+' '+items_left_text+'</div>';$input.parent().next().append(warning);$input.val(updated_quantity)}}})}
function refreshCart(cart, upd=0) {
    $("#shopify-section-header").load(location.href+" #shopify-section-header");
    if(typeof(BOLD) === 'object' && BOLD.helpers && typeof(BOLD.helpers.cleanCart) === 'function'){
        cart = BOLD.helpers.cleanCart(cart);
    }
    $cartBtn = $(".cart-button span");
    if ($cartBtn && $cartBtn.length) {
        var value = $cartBtn.text() || '0';
        var cart_items_html = "";
        var $cart = $("#cart ul");
        var cartItems = [];
        $cartBtn.text(value.replace(/[0-9]+/, cart.item_count));
        //$cart.find('li:not(:first)').remove();
        jQuery(".cart_shipping_block").hide();
        jQuery(".mm-subtitle.clearfix").hide();
        $('li.empty_cart').remove();
        $('li.cart_items').remove();
        if (cart.item_count == 0) {
            $cart.append('<li class="empty_cart">{{ 'layout.general.empty_cart' | t }}</li>')
        } else {
            jQuery(".cart_shipping_block").show();
            jQuery(".mm-subtitle.clearfix").show();
            updateFreeShipping(cart);
            cart_items_html += '<li class="cart_items scroll-section"><ul class="mm-listview">'; /*DB - 4/1/2020*/
            $.each(cart.items, function(index, item) {
                if (item.formatted_recurring_desc === undefined)
                item.formatted_recurring_desc = "";
                var line_id = index + 1;
                cart_items_html += '<li class="cart_item">' + '<p class="mm-counter">' + '<span class="icon-minus cart-minus"></span>' + '<input type="number" min="0" class="quantity" name="updates[]" id="updates_' + item.id + '" value="' + item.quantity + '"  data-line-id="' + line_id + '" readonly />' + '<span class="icon-plus plus"></span>' + '</p>' + '<a href="' + item.url + '">';
                if (item.image) {
                cart_items_html += '<div class="cart_image">' + '<img src="' + item.image.replace(/(\.[^.]*)$/, "_medium$1").replace('http:', '') + '" alt="' + htmlEncode(item.title) + '" />' + '</div>'
                }
                cart_items_html += '<div class="item_info">' + item.title + item.formatted_recurring_desc;
                if (item.properties) {
                $.each(item.properties, function(title, value) {
                    if (value) {
                    cart_items_html += '<div class="line-item">' + title + ': ' + value + '</div>'
                    }
                })
                }
                cart_items_html += '<div class="price"><span class="money">' + Shopify.formatMoney(item.price, $cart.data('money-format')) + '</span></div></div></a></li>'
                var onSale = (item.price < item.compare_at_price) ? true : false;
                var msrp = (item.price < item.compare_at_price) ? item.compare_at_price : item.price;
                cartItems.push({
                  id: item.id,
                  price: item.price,
                  quantity: item.quantity,
                  on_sale: onSale,
                  available: true,
                  msrp: msrp
                });
            });
            cart_items_html += '</ul></li>';

            $(cart_items_html).insertAfter(".cart_shipping_block");
            $cart.find('li.cart_item:last').addClass('last_cart_item');
          
          	$('.mm-counter.price .money').html(Shopify.formatMoney(cart.total_price, $cart.data('money-format')));
          
            {% if settings.show_multiple_currencies %}
            convertCurrencies();
            {% endif %}

            if( 
                typeof(BOLD) == "object" &&
                typeof(BOLD.common) == "object" &&
                typeof(BOLD.common.eventEmitter) == "object" &&
                typeof(BOLD.common.eventEmitter.emit) == "function" 
            ){
                BOLD.common.eventEmitter.emit('BOLD_COMMON_cart_loaded');
            }
            $('li.recommendations.cloned').remove();
            $('li.recommendations').clone().addClass('cloned').insertAfter(".cart_item.last_cart_item").show();
        }
        if(upd!=1) {
          delete window.IntentProperties.cart;
          window.IntentProperties.cart_count = cart.item_count;
          window.IntentProperties["cart"] = cartItems;
          window.intent.push({ trigger: 'refresh' });
          console.log(window.IntentProperties);
        }
    }
}

