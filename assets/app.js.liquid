      $('#stamped-file-uploader-input').attr('accept','.mov, .mp4,video/quicktime,video/mp4');
    }
    clearInterval(up);
  }, 2000);
});
/* End Reviews fix */

/* Blog fixed menu */
$(document).ready(function(){
  if($('#shopify-section-blog-template-glow').length || $('#shopify-section-blog-template-author').length || $('#shopify-section-article-template').length) {
    console.log('blog');
	var w = $(window).width();
	var lastScrollTop = 0;
    var scroll_start = 0;
    if(w < 768) {
      $(window).scroll(function(e) { 
        scroll_start = $(this).scrollTop();
        if(scroll_start > lastScrollTop || scroll_start == 0) {  // scroll down
          $('#shopify-section-blog-template-glow').removeClass('sticky');
          $('#shopify-section-blog-template-author').removeClass('sticky');
          $('#shopify-section-article-template').removeClass('sticky');
        } else { // scroll up
          $('.blog-cat-menu').hide();
          $('#shopify-section-blog-template-glow').addClass('sticky');
          $('#shopify-section-blog-template-author').addClass('sticky');
          $('#shopify-section-article-template').addClass('sticky');
        }
        lastScrollTop = scroll_start;
      });
      
    } else {
      $('#shopify-section-blog-template-glow').addClass('sticky');
      $('#shopify-section-blog-template-author').addClass('sticky');
      $('#shopify-section-article-template').addClass('sticky');
    }
  }
});
