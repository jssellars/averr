/* Attentive SMS Footer  */
/*****************************************************************************************
201: Request Successful, Subscriber has been created
200: Request Successful, Subscriber already exists, Subscriber successfully opted out
400: Bad Request (data is improperly formatted), No subscriber exists
401: Authorization Error (API Key is incorrect)
409: Subscriber already opted out
500: Attentive Server-Side Error
******************************************************************************************/
$(document).ready(function(){
  
  $('.footer-attentive-sms-text a').attr('target','blank');
  $('#attentive-sms-button').on('click', function(e){
    $('#attentive-sms-message').text('').hide();
    $('#attentive-sms-button').addClass('disabled').attr('disabled', '');
    var phoneNum = $('#attentive-sms-input').val();
    var attentiveVisitorId = document.cookie.replace(/(?:(?:^|.*;\s*)__attentive_id\s*=\s*([^;]*).*$)|^.*$/, "$1");
    console.log(phoneNum);
    console.log(attentiveVisitorId);
    $.support.cors = true;
    $.ajax({
      type: 'GET',               
      processData: false,
      contentType: false,
      data: "phone="+phoneNum+"&visitor="+attentiveVisitorId,
      url: "https://apps.sugatan.io/scripts/aa/smsAttentive.php" ,
      dataType: 'json',
      crossDomain: true,
      error: function(data){
    	$('#attentive-sms-button').removeClass('disabled').removeAttr('disabled');
        $('#attentive-sms-input').val('');
        $('#attentive-sms-message').addClass('error').text(data).show();
      },
      success: function(data){
        $('#attentive-sms-button').removeClass('disabled').removeAttr('disabled');
        $('#attentive-sms-input').val('');
        if(data == "The `phone` field is not valid"){
        	$('#attentive-sms-message').addClass('error').text("Invalid phone number!").show();
        /*}else if(data){
        	$('#attentive-sms-message').addClass('error').text(data).show();*/
        } else{
          $('#attentive-sms-message').addClass('success').text('Thanks for subscribing!').show();
        }
      }
    });
  });
});
